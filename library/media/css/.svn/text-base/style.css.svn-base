#tabs ul li.selected div { color:black; margin:0; background-color:#FEF8D8; border-color:#FCECAA; }
#tabs ul li.selected span.arrow { position:absolute; top:0; right:0; width:30px; height:44px; background:url(img/tab-arrow-30px.png) no-repeat top; display:block; }



/******************************************************************************\
    Specific Elements
\******************************************************************************/
