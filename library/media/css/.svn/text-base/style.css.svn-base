@charset "UTF-8";

/*  UCF Web Communications, Summer 2010*/

html, body{padding:0; margin:0}
html{background:url("img/gradient.png") repeat-x top left #fff}
a img{border:0}

.uppercase{text-transform:uppercase}

.navigation_end{margin-bottom:-10px; clear:both}
#navigation{margin:25px auto 0 auto; position:relative; left:5px; font-family:Georgia,serif; font-size:95%; width:802px; padding:0; list-style-type:none}
#navigation li{float:left; margin:0 0 0 11px; padding:0 0 0 13px; background-image:url(img/bullet.png); background-position:center left; background-repeat:no-repeat}
#navigation li.first{margin-left:0; padding-left:0; background-image:none}
#navigation #nav_apply_now{text-indent:-9999px; background-image:none}
#navigation #nav_apply_now a{cursor:pointer; display:block; overflow:hidden; background-position:top center; background-repeat:no-repeat; background-image:url(img/apply_now.png); width:93px; height:24px; position:relative; left:-5px; top:-2px}

#navigation li a{color:#262626; text-decoration:none}
#navigation li a:hover{color:#959491}

#main_body a{color:#600; text-decoration:none; border-bottom:#CCC}
#main_body a:hover{color:#e50606; text-decoration:none; border-bottom:#e50606}

#footer{clear:both; text-align:center; font-family:Arial,Helvetica,sans-serif; padding-top:30px; letter-spacing:1px}
#footer p{font-size:0.63em; color:#4f4e4e; padding:0; margin:0 0 2px 0}
#footer p a, #footer p a:visited{color:#000}
#footer_navigation{font-size:0.63em; margin-bottom:7px}
#footer ul{list-style:none}
#footer ul li{display:inline; border-left:#000 1px solid; padding:0 5px 0 8px}
#footer ul li.first{border:0; padding-left:0px}
#footer ul a, #footer ul a:visited{color:#3e3d3d; text-decoration:none}
#footer ul a:hover{text-decoration:underline}
#ucf_address{font-size:0.63em; color:#4f4e4e; padding:0; margin-top:5px; margin-right:0; margin-bottom:0; margin-left:0}
#footer a#ucf_logo{display:block; height:43px; width:256px; text-indent:-9000px; background:url(img/ucf_stands_for_opportunity.png) no-repeat; margin:25px auto}


/******************************************************************************\
    General Styles
\******************************************************************************/
body { font-family: "Trebuchet MS", sans-serif; font-size:90%; text-align:left; }
#wrap { width:950px; padding:24px 0 32px; margin:0 auto; }
abbr {border:1px dash black; outline:0; }
a { color:#990000; }
a:hover { cursor:pointer; color:#C30202; }
.inline { display:inline; }
.borderless { border:0; }
.hidden { display:none; }
.orange { color:#ff7700; }


h1 { font-family:georgia, serif; font-size:2em; }
h1 a {text-decoration:none; color:black; }
h1 a:hover {color:black; }
h2 { /*color: #FF7700;*/ color:#f3c00b; font-family: "Arial Black", arial, sans-serif; font-size:1.6em; margin:0; padding:0 0 .5em 0; }
h3 { letter-spacing:.5px; font-size:1.15em; font-family: "Arial Black", arial, sans-serif; padding:.5em 0; margin:0; }
h4 {font-weight:bold; font-size:1em; padding:.5em 0; margin:0; font-family:georgia; }

.ribbon { background:url("img/white-triangle.png") no-repeat scroll left center #CC9900;
		color:white; font-size:1em; font-weight:bold; height:40px; line-height:40px; margin-bottom:1.5em; padding:0;
		padding-left:45px; }
.ribbon a { color:white; text-decoration:none; border-bottom:1px dotted; }
.ribbon a:hover { border-bottom:1px solid; }

ul, ol { font-family:georgia; }
li { line-height:120%; padding-bottom:1em; } 
ul.checkboxes { margin-top:0; padding-top:0; padding-left:0px; }
ul.checkboxes li {list-style:none; display:block; clear:both; padding:5px 0;  }
p { font-family:georgia; padding-bottom:1em; margin-bottom:0; line-height:150%; }
address { padding-left:2em; font-style:normal; font-family:georgia; }
address a { color:black; text-decoration:none; border-bottom:1px dotted black; }

a[target="blank"], a[target="_blank"] { vertical-align:text-bottom; display:inline-block; line-height:18px; background:url(img/new-window.png) right top no-repeat; padding-right:16px; overflow:hidden; }
a[target="blank"]:hover, a[target="_blank"]:hover { background-position: right -50px; }
.ribbon a[target="blank"], .ribbon a[target="_blank"] { background-position: right -100px; } 
a[href^="tel"] { color:black; text-decoration:none; }
a[href^="tel"]:hover { cursor:default; }
.pdf { display:inline-block; line-height:18px; background:url(img/document-pdf-text.png) right top no-repeat; padding-right:20px; overflow:hidden; }

.indent { padding-left:2em; }
.indent ul { padding-left:1em; font-family:georgia; }
.upfont { font-size:123%; }

.plain{font-family: "Georgia"; list-style-type: none; padding: 0; margin: 0;}
.plain li {}

/******************************************************************************\
    sans serif content
\******************************************************************************/
.sans { font-size:.9em; font-family:helvetica, sans-serif; padding:0 0 1em 1em; }
.sans p { font-family:helvetica, sans-serif; padding-bottom:.5em; }
.sans h3 { font-family:helvetica, sans-serif; font-weight:bold; }
.sans h4 { font-family:helvetica, sans-serif; font-size:1em; padding-bottom:0;}
figure img { -webkit-box-shadow: rgba(0, 0, 0, 0.3) 0px 0px 6px; -moz-box-shadow: rgba(0, 0, 0, 0.3) 0px 0px 6px;  }
.ie figure img { border:1px solid #CCC; }
figure { padding:.5em 0 1.5em; }


/******************************************************************************\
    Why UCF
\******************************************************************************/
#why-ucf .frame { margin:1.2em 0 .4em; }
#why-ucf .float { float:left; margin-right:5px; }


/******************************************************************************\
    Campus Life
\******************************************************************************/
#campus-life img { display:block; margin-bottom:.5em; -webkit-box-shadow: rgba(0, 0, 0, 0.3) 0px 0px 6px; -moz-box-shadow: rgba(0, 0, 0, 0.3) 0px 0px 6px; background-color: white; -moz-border-radius: 5px; -webkit-border-radius: 5x; border-radius:5px; padding:5px; position:relative; }
#campus-life a { display:block; color:#f90404; text-decoration:none; padding-bottom:1em; font-weight:bold; font-size:1.1em; font-family: georgia; overflow:visible; }
#campus-life a[target="blank"] { display:block; background:url(none); padding-right:0; }
#campus-life a span { display:inline-block; line-height:120%; background:url(img/new-window.png) right -50px no-repeat; padding-right:16px; overflow:hidden; }
#campus-life a span:hover { background-position: right -50px; }
#campus-life a:hover { color:#ff6d03; }
#campus-life a:hover img { -webkit-box-shadow: rgba(255, 109, 3, 0.3) 0px 0px 6px; -moz-box-shadow: rgba(255, 109, 3, 0.3) 0px 0px 6px; }
.ie #campus-life a { height:170px; }
.ie #campus-life img { border:1px solid #CCC;  }
.ie #campus-life a:hover img { border:1px solid #FFAE76;  }


/******************************************************************************\
    Homepage Slideshow
\******************************************************************************/
#slideshow { position:relative; height:325px; padding-bottom:2em; margin-top:.5em; }
#slideshow img { position:absolute; top:0; left:0; z-index:8; opacity:0.0; }
#slideshow img.active { z-index:10; opacity:1.0; }
#slideshow img.last-active { z-index:9; }


/******************************************************************************\
  FAQs
  Styled/scripted like an accordion
\******************************************************************************/
.accordion { font-family:georgia, serif; border-top:1px solid #FFD333; padding-bottom:1em; }
.accordion dt { padding:5px; color:black; border-bottom:1px solid #ffd333; font-weight:normal; font-size:1em; }
.accordion dt:hover{ background-color:#fffae8; cursor:pointer; }
.accordion dd { display:none; padding:0 1.5em 1em; border-bottom:1px solid #ffd333; margin:0; }
.accordion dd h4 { padding-bottom:0; font-style:italic; }
.accordion dt.open{ color:black; border-bottom:none; background-image: url(img/accordian-close.gif); background-repeat: no-repeat; background-position: right center; background-color:#fffae8; }


/******************************************************************************\
  Spotlights
\******************************************************************************/
#statistics-spotlight{text-align: center; font-family: "Arial", sans-serif;}
#statistics-spotlight .section {margin-bottom: 15px;}
#statistics-spotlight h3 {font-size: 15px; font-family: "Arial Black", "Arial", sans-serif; font-weight: bold; padding-bottom:0;}
#statistics-spotlight h4 { font-family:arial,sans-serif; margin:0; padding:.5em 0 0 0; }
#statistics-spotlight ol{text-align: left; padding-left: 3em;}
#statistics-spotlight #population {font-size: 34px; display: block; margin-bottom: 15px;}
#statistics-spotlight .sat {margin-bottom: 5px;font-size: 34px; display: block;}
#statistics-spotlight #from-counties,
#statistics-spotlight #from-states,
#statistics-spotlight #from-countries {font-family: "Arial", sans-serif; font-size: 14px;}
#statistics-spotlight .number{color: #0a88b1; font-family: "Georgia", serif; line-height: 1em;}

#pub-spotlight {}
#pub-spotlight a { text-decoration:none; color:black; }
#pub-spotlight a strong { display:block; font-family:"Trebuchet MS",sans-serif; font-size:14px; }
.col #pub-spotlight { padding-bottom:15px; text-align:center; }



/******************************************************************************\
  Media Center
	- jquery tools overlay for video
	- shadowbox for publications (copied code from today.ucf)
\******************************************************************************/
.carousel { position:relative; padding:0 0 25px 0; margin:0 0 2em 0; }
.carousel ul { margin:0; padding:0; }
.carousel li { list-style:none; }
.carousel-control { z-index:10; position:absolute; display:block; height:36px; width:36px; color: #FF7700;  bottom:0; text-decoration:none; background-image:url(img/carousel-controls.png); text-indent:-500px; overflow:hidden; }
.carousel .next { right:10px;  }
.carousel .next:hover { background-position:0 -72px; }
.carousel .previous { left:10px; background-position:0 -36px;}
.carousel .previous:hover { background-position:0 -108px; }
.carousel-pagination { position:absolute; text-align:center; left:0; bottom:0; width:100%; }
.carousel-pagination p { padding-bottom:5px; }
.carousel-pagination a { display: inline-block; margin-right: 5px; width: 10px; height: 10px; background: #ffbb77; text-indent: -1000px; overflow: hidden; -webkit-border-radius: 5px; -moz-border-radius: 5px; }
.carousel-pagination a.active { background: #ff7700; }

.ie7 .pub-car .carousel-pagination p { padding-bottom:0; }
.ie7 .carousel-pagination { position:relative; width:auto; margin-left:340px;  }
.ie7 .carousel-pagination a { display:block; float:left; margin-right:5px; }
.ie6 .carousel-control { position:relative; width:auto; height:auto; text-decoration:underline; text-indent:0; background:none; cursor:pointer; font-weight:bold; padding:5px 0; }
.ie6 .carousel { position:relative; padding:1px; border:1px solid #DADADA; }
.ie6 .carousel .next { right:auto; display:block; left:10px; }
.ie6 .carousel ul li { margin:0; margin-bottom:0; }
.ie6 .carousel-pagination { display:block; position:relative; }


.video { width: 220px; float:left; padding:5px 15px 0 5px; font-size:12px; }
.video h4 { font-size:12px; font-wight:bold; padding:7px 0 3px 5px; margin:0; }
.video p { line-height:120%; padding:0 5px 15px; margin:0; }
.video .icon {  -webkit-box-shadow: rgba(0, 0, 0, 0.3) 0px 0px 6px; -moz-box-shadow: rgba(0, 0, 0, 0.3) 0px 0px 6px; background-color: white; -moz-border-radius: 5px; -webkit-border-radius: 5x; border-radius:5px; width:210px; height:118px; padding:5px; position:relative; }
.video .icon:hover { -webkit-box-shadow: rgba(255, 109, 3, 0.3) 0px 0px 6px; -moz-box-shadow: rgba(255, 109, 3, 0.3) 0px 0px 6px; }
.ie .video .icon:hover { border:1px solid #FFAE76;  }

.video .icon img { width:210px; height:118px; }
.video .icon .play { display:block; background: url(img/video-play.png) no-repeat center center; width:210px; height:118px; position:absolute; top:5px; left:5px; z-index:10; }
.video-large-group { padding-bottom: 20px; border-bottom: dotted 1px black; margin-bottom: 20px; }
.video-large-wrapper { -webkit-box-shadow: rgba(0, 0, 0, 0.3) 0px 0px 6px; -moz-box-shadow: rgba(0, 0, 0, 0.3) 0px 0px 6px; background-color: white; -moz-border-radius: 8px; -webkit-border-radius: 8px; border-radius:8px; width:442px; height:250px; padding:5px; margin-right:25px; float:left; }
.video-large {width:342px; height:250px; background: black url(img/video-large-bg.gif) no-repeat center center; }
.video-large-overlay { width:342px; height:250px; background: transparent url(img/video-large-overlay.png) no-repeat center center; }
.ie .video .icon { border:1px solid #CCC; }
.ie6 .video .icon .play { background-image:none; } /* transparent png */


#downloads { padding-top:10px; }
#downloads p span { font-size:80%; display:block; }
#downloads img { -webkit-box-shadow: rgba(0, 0, 0, 0.3) 0px 0px 6px; -moz-box-shadow: rgba(0, 0, 0, 0.3) 0px 0px 6px; }
#downloads ul { margin:0; padding:0; }
#downloads li { list-style:none; width:260px; margin:0; padding:0 20px 0 0; float:left; }



/* the overlayed element */
.apple_overlay { display:none; background-image:url(img/overlay-white.png); width:640px; padding:35px; font-size:11px; }
.apple_overlay .close { background-image:url(img/overlay-close.png); position:absolute; right:20px; top:20px; z-index:1000; cursor:pointer; height:35px; width:35px; }
#overlay { background-image:url(img/overlay-bg.png); color:#efefef; height:604px; width:1068px; padding: 50px 46px; z-index:100; }
div.contentWrap { height:705px; overflow-y:auto; }

#publications { padding-top:.75em; }
#publications ul { margin:0; padding:0; }
#publications li { display: inline; list-style-type: none; width:170px; float:left; height:250px; padding:0 10px .0 5px; }
.ie #publications li { padding-right:15px; }
#publications li img, .pub img{ -webkit-box-shadow: rgba(0, 0, 0, 0.3) 0px 0px 6px; -moz-box-shadow: rgba(0, 0, 0, 0.3) 0px 0px 6px; }
#publications li span, .pub span { width:160px; clear:both; float:left; text-decoration:none; color:black; }
#publications a:hover span, .pub a:hover span { color:#C7A729; }
.pub { padding:1em 0 1.5em 33px; }
.pub span { font-family:georgia; display:block; float:none; line-height:125%; }







/******************************************************************************\
    Generic Form Styling
\******************************************************************************/
.QAPanel { border-bottom: 1px solid #ECDC9A; padding:1.2em 1em .8em; }
.QAPanel .QAPanel { border-bottom:0; padding:0; margin:0; }
.Questions .even {  }
.QText { font-weight:bold; font-size:1.1em; }
.QAns  { padding-bottom:.4em; }
/*.QAPanel label {display:block;  }*/
.QAPanel h3 {padding-bottom:.3em; }
.QAPanel label.checkbox {padding-top:5px; }
.QAPanel select { padding:3px; }
.QAPanel input[type=text] { padding:3px; }
.QAPanel > input[type=text] { min-width:300px; }
.QAPanel input[type=password] { padding:3px; }
.QAPanel table { width:auto; border-collapse:collapse; margin:.5em 0 1em; }
.QAPanel table th { margin:0; padding:3px 5px; background:#fcecaa; border:1px solid #8D8D8D; }
.QAPanel table td { margin:0; padding:3px 5px; border:1px solid #C4C4C4; background:#FCFCF4; }
.QAPanel table td.no-border { border:0; }
.QAPanel h4 {padding-bottom:5px; margin-bottom:0; }
.QAPanel .warningText { padding:5px; margin-top: 10px; margin-bottom: 10px; background:#ffe0e0; border:1px solid black; font-style:italic; color:Red; }
.QAPanel .message { margin-bottom:.25em; }
.QAPanel .listAddArea { padding:3px; border:1px solid #D8DCDF; background-color:#eeeeee; } /*mimics ui-button styling*/
.QAPanel .qhelp { display:inline; }

/******************************************************************************\
    Block Styles
\******************************************************************************/

/* html5 elements */
article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section { display: block; }


.frame { display:block; margin-bottom:1em; -webkit-box-shadow: rgba(0, 0, 0, 0.3) 0px 0px 6px; -moz-box-shadow: rgba(0, 0, 0, 0.3) 0px 0px 6px; background-color: white; -moz-border-radius: 5px; -webkit-border-radius: 5x; border-radius:5px; padding:5px; position:relative; }
.frame { -moz-box-shadow: rgba(0, 0, 0, 0.3) 0px 0px 6px; }
.ie .frame { border:1px solid #CCC; }


.paper { margin-bottom:2em; background: url("img/paper-top.png") no-repeat center top transparent; font-size:.83em; font-family:helvetica, sans-serif; }
.paper p { font-family:helvetica, sans-serif; line-height:170%; }
.paper h3 {font-size:100%; }
.paper .blue { color:#105d8a; font-family:georgia; font-size:1.6em; margin:0; padding:.1em 0 .25em; }
.paper .wrap { background: url("img/paper-bottom.png") no-repeat center bottom transparent; padding:1em; }
.paper ul { margin:0; padding:0; }
.paper ol { padding-left:1.5em; }
.paper li { margin:.5em 0 .5em 1.2em; padding:0; line-height:130%;  }
.paper .video { width:100%; overflow:hidden; padding:5px 0 8px; position:relative;  }
.paper .video:hover { cursor:pointer; }
.paper .video .play { display:block; background: url(img/video-play.png) no-repeat center center; width:202px; height:126px; position:absolute; top:5px; left:0; z-index:10; } 


.callout { margin-bottom:2em; padding:4px 8px 12px; font-family:helvetica, sans-serif; background:#fffae9; font-size:.83em; -webkit-box-shadow: rgba(0, 0, 0, 0.3) 0px 0px 6px; -moz-box-shadow: rgba(0, 0, 0, 0.3) 0px 0px 6px; }
.callout h3 { color:#d6aa15; font-family: "Arial Black", arial, sans-serif; font-size:1.26em;}
.callout ul { margin:0; padding:0; }
.callout li { margin:0; padding:0 0 1em; list-style:none; }
.ie .callout { border:1px solid #F3E7DC;}

.video-side { margin-bottom:2em; }
.video-side h3 { color: #FF7700; }
.video-side .video h4 { font-family: "Trebuchet MS", sans-serif;  }



/* Estimated Cost Table */

#estimated-cost { -webkit-box-shadow: rgba(0, 0, 0, 0.3) 0px 0px 4px; -moz-box-shadow: rgba(0, 0, 0, 0.3) 0px 0px 4px; background: #B5C365 url(img/cost-green-bg.jpg) no-repeat center center; -moz-border-radius: 8px; -webkit-border-radius: 8px; border-radius:8px; padding:13px; line-height: 1.2em; margin-bottom: 1em;}
#estimated-cost.nobg {background: none; background-color: #B5C365;}
#estimated-cost-dollar { background: transparent url(img/cost-dollar-bg.png) no-repeat center center;}
#estimated-cost.nobg #estimated-cost-dollar {background: none;}
#estimated-cost table { border-collapse: collapse;}
#estimated-cost h3 { color: white;}
#estimated-cost table, #estimated-cost h3 { margin-bottom: .5em;}
#estimated-cost em { font-size: 10px; }
#estimated-cost p { line-height:90%; padding:0 0 .3em 0; margin:0;}
#estimated-cost table caption{ background: none; padding: 0px; line-height: 1.2em; font-weight:bold;}
.cost-total { font-weight: bold; border-top: 1px solid black;}
#estimated-cost table td{ padding:0; font-size:13px; line-height: 1.2em;}


/******************************************************************************\
    Visit UCF
\******************************************************************************/
.tan   { background:#fdfae1;  }
.tan .wrap { padding:1.2em; }
.tan h4 { color:#bf0f0f; padding-top:0; margin-top:0; font-family:arial; font-size:1em; font-weight:bold;}
.tan ul { margin:0; padding:0 0 0 1.4em; }
.tan li { margin:0; padding:0; }
.eq-height { padding-top:.5em; }
.eq-height .same { position:relative; min-height:355px;}
.eq-height .same .tan { position:absolute; min-height:220px; bottom:0;}
.green { display:inline-block; border:1px solid #128f18; -moz-border-radius: 4px; -webkit-border-radius: 4px; border-radius: 4px; padding:.7em; background:#2E9C20; color:white; text-decoration:underline; margin:.3em 0 0; font-family:georgia; }
.green:hover { color:white; background: #28A410; }
.green-block { display:block; width:212px; text-align:center; padding:.7em 0; margin:0; background:#128f18; color:white; text-decoration:underline; font-family:georgia; margin-bottom:1em;  }
.green-block:hover { color:white; }



/******************************************************************************\
    Left-side tabs
\******************************************************************************/
#tabs ul { margin:0; padding:0; }
#tabs ul li { position:relative; list-style:none; margin:0; padding:0 6px 0 0; }
#tabs ul li span.arrow {display:none;}
#tabs ul li div { background:white; padding:8px; margin:4px 0; border:1px solid #FBF2C6; color:#707070; -moz-border-radius: 4px; -webkit-border-radius: 4px; }
