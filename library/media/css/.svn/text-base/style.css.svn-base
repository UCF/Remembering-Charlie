#ready-checklist li { background: transparent url(img/ready-green-check.gif) no-repeat left center; padding: 0 0 .5em 23px; }
#ready-checklist li a{ color: #009933; font-size: 14px; font-weight: bold; text-decoration: none; }
#ready-checklist li a:hover { text-decoration: underline; }

#map_canvas { margin:1em 0 2em; }

.vertical-rule { height:180px; border-left:1px solid #dacc93; }

#links {padding:2em 0;}
