/******************************************************************************\
    Layout
\******************************************************************************/
#header { border-bottom:1px dotted black; padding-bottom:1em; margin-bottom:1em; }
#header h1 { font-weight:normal; margin:0; padding:0;  }
#header ul { list-style:none; margin:0; color:#6d6e71; padding:.28em 0 0 0; }
#header ul li{ display:inline-block; float:left; margin-right:8px; padding:0; line-height:160%; }
#header ul li:after { content:"\2022"; margin-left:5px; }
#header ul li.last:after{ content:""; margin-left:0; }
#header ul li a { font-family: Georgia, Times, serif; margin-right:5px; color:#333; font-size:16px; text-decoration: none; font-weight: normal; }
#header ul li a:hover {  color:#ed1c24;  text-decoration: underline; }

.pad-l { padding-left:1em; }
.pad-r { padding-right:1em; }

#footer { width: 100%; background:black; margin:0; text-align:left; color:white; padding:1.5em 0; font-size:80%; }
#footer h2 { font-size:1.2em; margin:0; padding:0; color:#ffcc00;  }
#footer h2 a { color:#ffcc00;  }
#footer h2 a:hover { color:#C30202; }
#footer h3 {color:white; font-size:1em; font-weight:bold; margin:0; padding:.2em 0 0 0; }
#footer a { color:white; }
#footer a:hover { color:#C30202; }
#footer a[href^="tel"]:hover { color:white; }
#footer a[target="blank"] { background-position: right -100px; }
#footer a[target="blank"]:hover { background-position: right -50px; }
#footer dl, #footer dt, #footer dd, #footer ul, #footer ul li { margin:0; padding:0; color:white; }
#footer dt {color:#ff6d02; font-style:italic; font-weight:normal; padding-top:.5em;  }
#footer ul { margin:0; padding:0; font-family: arial, sans-serif; }
#footer ul a {color:white; }
#footer ul a:before { content:"Â» "; }
#footer ul a:visited  {color:white; }
#footer ul a:hover {color:white; cursor:pointer; color:#C30202; }
#footer ul li {margin:0; padding:0; display:block; }
#footer ul li.hr {display:block; border-bottom:1px dotted white; overflow:hidden; background:black; padding-bottom: 5px; margin-bottom: 5px;}
#footer dl { margin:0; padding:0; color:white; }
#footer .wrap {width:950px; margin:0 auto; background:black; }


